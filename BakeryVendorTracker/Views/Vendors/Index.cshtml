<link rel="stylesheet" type="text/css" href="~/css/styles.css" />

@using BakeryVendorTracker.Models;

@if (@Model.Count == 0)
{
  <h3>No Vendors Have Been Add To This List</h3>
}

@foreach (Vendor vendor in Model)
{
  <h3><a href='/vendors/@vendor.VendorId'>@vendor.VendorName</a></h3>
}

<p><a href='/'>Back home</a></p>